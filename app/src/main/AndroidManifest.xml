<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.curl.example" >
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.curl.example.MyActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.curl.example.ExampleActivity1"
            android:label="文字" >
        </activity>
        <activity
            android:name="com.example.curl.example.ExampleActivity2"
            android:label="图片" >
        </activity>
        <activity
            android:name="com.example.curl.example.ExampleActivity3"
            android:label="自定义background" >
        </activity>
        <activity
            android:name="com.example.curl.example.ExampleActivity4"
            android:label="自定义border" >
        </activity>
        <activity
            android:name="com.example.curl.example.ExampleActivity5"
            android:label="无限翻页" >
        </activity>
        <activity
            android:name="com.example.curl.example.ExampleActivity6"
            android:label="异步加载文字" >
        </activity>
        <activity
            android:name="com.example.curl.example.ExampleActivity7"
            android:label="异步加载图片" >
        </activity>
        <activity
            android:name="com.example.curl.example.ExampleActivity8"
            android:label="异步加载图片(LruCache缓存)" >
        </activity>
    </application>

</manifest>
